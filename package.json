{
  "name": "css-press",
  "homepage": "https://webtoknow.github.io/CSS-Press",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "predeploy": "tsc && vite build --base=/CSS-Press/",
    "deploy": "gh-pages -d dist",
    "predeploy:test": "tsc && vite build --base=/cs-test/",
    "deploy:test": "gh-pages -d dist -r git@github.com:webtoknow/cs-test.git"
  },
  "dependencies": {
    "@xstate/react": "4.1.1",
    "animate.css": "^4.1.1",
    "html-react-parser": "5.1.10",
    "normalize.css": "8.0.1",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "6.24.1",
    "react-toggle": "4.1.3",
    "style-it": "^2.1.4",
    "xstate": "5.14.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@types/jest": "29.5.12",
    "@types/puppeteer": "7.0.4",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-toggle": "4.0.5",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "6.1.1",
    "puppeteer": "22.14.0",
    "sass": "1.77.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.3",
    "vite-plugin-svgr": "4.2.0",
    "vitest": "^2.0.4"
  }
}
