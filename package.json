{
  "name": "css-press",
  "homepage": "https://webtoknow.github.io/CSS-Press",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "tsc && vite build --base=/CSS-Press/",
    "deploy": "gh-pages -d dist",
    "predeploy:test": "tsc && vite build --base=/cs-test/",
    "deploy:test": "gh-pages -d dist -r git@github.com:webtoknow/cs-test.git"
  },
  "dependencies": {
    "@xstate/react": "4.1.1",
    "animate.css": "^4.1.1",
    "html-react-parser": "5.1.10",
    "normalize.css": "^8.0.1",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "6.23.1",
    "style-it": "^2.1.4",
    "xstate": "5.13.2"
  },
  "devDependencies": {
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-toggle": "4.0.5",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "6.1.1",
    "sass": "^1.77.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-svgr": "^4.2.0"
  }
}